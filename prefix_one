#include <iostream>
using namespace std;
int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	int t;
	cin>>t;
	while(t--)
	{
		long long int n;
		cin>>n;
		long long int a[n+1];
		for(int i=1;i<=n;i++)
		{
         cin>>a[i];
		}
		long long int prefix[n+1]={0};
		prefix[1]=a[1];
		for(int i=2;i<=n;i++)
		{
			prefix[i]=prefix[i-1]+a[i];
		}
		long long int q;
		cin>>q;
		while(q--)
		{
			long long int l,r;
			cin>>l>>r;
			if(l==1)
			{
			cout<<prefix[r]<<"\n";
			}
			else
			{
			cout<<prefix[r]-prefix[l-1]<<"\n";
			}
		}
	}
}
